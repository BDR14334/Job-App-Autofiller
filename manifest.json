{
    "manifest_version": 3,
    "name": "My Extension",
    "description": "Automatically fill Workday job application forms",
    "icons": {
        "32": "icon8.png"
    },
    "version": "1.0.0",
    "background": {
        "service_worker": "welcome/preface.js"
    },
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "storage",
        "activeTab",
        "declarativeContent",
        "scripting",
        "windows"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
    
}
